
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>
</head>
<body>
    <%- include("common/nav") -%>

    <h1>Profile Page</h1>
    <h2>Welcome ! <%=name%></h2>
    <h3>Email : <%=data.email%></h3>
    <h3>Age : <%=data.age%></h3>
    <h3>Gender : <%=data.gender%></h3>
    <ul>
        <% data.skills.forEach(function(items){ %>
            <li><%=items%></li>    
        <%})%>
    </ul>

</body>
</html>